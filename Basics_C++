// library
#include <iostream>

using namespace std;

auto fungsi () {
    int x = 0 ;
    for (int i = 1 ; i <= 10 ; i++) {
        x += 2 ;
    }
    return x ;
}

int fibo(int a) {
    //base case
    if (a <= 1) {
        return 1 ;
    } else {
        return fibo(a-1) + fibo(a-2) ;
    }
}

